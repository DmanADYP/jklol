@model IEnumerable<TOProjects.Models.LeadNote>

@{
    ViewBag.Title = "Notes";
    Layout = "~/Views/Shared/_LayoutII.cshtml";
}

<h2>Notes</h2>

<p>
    @Html.ActionLink("Create New", "Create",null, new { @class = "AppendParent" })
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Note)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
   <tr>
       <td>
           @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "AppendParent" }) |
           @Html.ActionLink("Details", "Details", new { id = item.Id }, new { @class = "AppendParent" }) |
           @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "AppendParent" })
       </td>

   </tr>
     <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Note)
        </td>
      
    </tr>
}

</table>
