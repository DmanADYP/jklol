<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - TO Projects</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Lead Tracker+", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">


                    <li class="dropdown">
                        <a class="dropdown-toggle" role="button" aria-expanded="false" aria-haspopup="true" href="#" data-toggle="dropdown">Projects<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li> @Html.ActionLink("Projects", "Index", "Projects")</li>
                                </ul>
                        </li>

                    <li class="dropdown">
                        <a class="dropdown-toggle" role="button" aria-expanded="false" aria-haspopup="true" href="#" data-toggle="dropdown">Leads<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li> @Html.ActionLink("Leads", "Index", "Leads")</li>
                            <li> @Html.ActionLink("Lead Sources", "Index", "LeadSources")</li>
                            <li> @Html.ActionLink("Lead Status", "Index", "LeadStatus")</li>
                            <li> @Html.ActionLink("Lead Types", "Index", "LeadTypes")</li>
                        </ul>
                    </li>

                    <li class="dropdown">
                        <a class="dropdown-toggle" role="button" aria-expanded="false" aria-haspopup="true" href="#" data-toggle="dropdown">Customers<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li> @Html.ActionLink("Customers", "Index", "Customers")</li>  
                            <li> @Html.ActionLink("Contacts", "Index", "Contacts")</li>
                            <li> @Html.ActionLink("Customer Contacts", "Index", "CustomerContacts")</li>

                                                  </ul>
                    </li>


                    <li class="dropdown">
                        <a class="dropdown-toggle" role="button" aria-expanded="false" aria-haspopup="true" href="#" data-toggle="dropdown">Employees<span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li> @Html.ActionLink("Employees", "Index", "Employees")</li>

                            <li> @Html.ActionLink("Employee Types", "Index", "EmployeeTypes")</li>

                        </ul>
                    </li>


                        </ul>
                
</div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - TO Projects</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")

    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css" />
    <link rel="stylesheet"  href="~/Scripts/jquery.simple-dtpicker.css" />
    <link rel="stylesheet" href="~/Scripts/chosen.css" />


    
     <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>


    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @*change to 653 when updating it 654,655 ect*@
    @Scripts.Render("~/Scripts/ToProjects.js?l=662")

    @Scripts.Render("~/Scripts/jquery.simple-dtpicker.js?tz=58")
 

    @Scripts.Render("~/Scripts/chosen.jquery.js")


</body>
</html>
